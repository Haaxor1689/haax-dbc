{
  "name": "haax-dbc",
  "version": "1.3.1",
  "author": "Haaxor1689",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Haaxor1689/haax-dbc.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "haax-dbc": "./dist/cli.mjs"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "bundle": "bun build ./src/index.ts --compile --minify --outfile haax-dbc",
    "build": "tsup",
    "prepublish": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "^1.2.4",
    "typescript": "^5.8.2",
    "tsup": "^6.7.0",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "commander": "^11.1.0"
  },
  "dependencies": {
    "@haaxor1689/nil": "^1.0.3"
  }
}
